    "image_name": "bash-py-postgres",
    "init_args": {
        "work_dir": "/workspace",
        "ports": {
            "5432": "12001"
        }
    },
    "instruction": "Our restaurant had unusually low traffic last Tuesday. We would like to look at the weather for that day to see if that was a contributing factor.  What was the temperature last Tuesday in Hardwick, Massachusetts? ",
    "hardness": "level2",
    "config": [
        {
            "type": "download_and_execute",
            "parameters": {
                "url": "https://drive.usercontent.google.com/download?id=1EFbupAElaIWSdPZ1Pw4e3mJyLrUoRiFm&export=download&authuser=0&confirm=t",
                "path": "/workspace/init.sh"
            }
        }
    ],
    "evaluator": {
        "func": "compare_csv",
        "result": {
            "type": "docker_file",
            "path": "/workspace/result.csv"
        },
        "expected": {
            "type": "cloud_file",
            "path": "https://drive.usercontent.google.com/download?id=1I1vNz41nq0EsGGzdXODdf3J--cxfcAfZ&export=download&authuser=0&confirm=t",
            "dest": "/workspace/gold.csv"
        }
    }
}