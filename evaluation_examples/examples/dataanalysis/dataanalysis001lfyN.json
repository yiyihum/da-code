{
    "id": "dataanalysis001lfy",
    "source": "https://www.kaggle.com/datasets/nitishsharma01/olympics-124-years-datasettill-2020/code",
    "image_name": "dabench-postgres-image",
    "init_args": {
        "work_dir": "/workspace",
        "ports": {
            "5432": "12001"
        }
    },
    "instruction": "From the 'Olympics 124 years Dataset (till 2020)',  conduct an analysis to determine the total number of Games held for both the Summer and Winter Olympics, and record this information in 'allGames.csv'. Additionally, for each edition of both the Summer and Winter Olympics, identify the host city and the total number of events conducted. Compile this detailed information into 'allEvents.csv'.",
    "hardness": "level1",
    "config": [
        {
            "type": "copy",
            "parameters": {
                "files": [
                  {
                    "url": "./benchmark/source/dataanalysis001lfyN/regions.csv",
                    "path": "/workspace/regions.csv"
                  },
                  {
                    "url": "./benchmark/source/dataanalysis001lfyN/Athletes_winter_games.csv",
                    "path": "/workspace/Athletes_winter_games.csv"
                  },
                  {
                    "url": "./benchmark/source/dataanalysis001lfyN/Athletes_summer_games.csv",
                    "path": "/workspace/Athletes_summer_games.csv"
                  }
                ]
            }
        },
        {
            "type": "copy_execute",
            "parameters": {
                "url": "./benchmark/source/dataanalysis001lfyN/init.sh",
                "path": "/workspace/init.sh"
            }
        }
    ],
    "evaluator": {
        "postconfig": [
            {
              "type": "copy",
              "parameters": {
                "files": [
                    {
                      "url": "./benchmark/source/dataanalysis001lfyN/gold",
                      "path": "/workspace/gold"
                    }
                  ]
              }
            }
        ],
        "func": [
            "compare_csv",
            "compare_csv"
        ],
        "result": [
            {
                "type": "docker_file",
                "path": "/workspace/allGames.csv"
            },
            {
                "type": "docker_file",
                "path": "/workspace/allEvents.csv"
            }
        ],
        "expected": [
            {
                "type": "docker_file",
                "path": "/workspace/gold/allGames.csv"
            },
            {
                "type": "docker_file",
                "path": "/workspace/gold/allEvents.csv"
            }
        ],
        "conj": "and"
    }
}