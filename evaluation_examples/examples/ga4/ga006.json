{
    "id": "ga006",
    "source": "https://developers.google.com/analytics/bigquery/advanced-queries?hl=en",
    "image_name": "bash-py-postgres",
    "init_args": {
        "work_dir": "/workspace",
        "ports": {
            "5432": "12001"
        }
    },
    "instruction": "I want to know the average amount of money spent per session by each user. This database is very large and you may need to understand the table schema somehow. These are the results corresponding to November and December 2020. Store the answer in 'result.csv'.",
    "hardness": "level2",
    "config": [
        {
            "type": "download_and_execute",
            "parameters": {
                "url": "https://drive.usercontent.google.com/download?id=1uRJT6e3DxkUdgpG_FEGUkNKx6SSQ04RP&export=download&authuser=0&confirm=t",
                "path": "/workspace/init.sh"
            }
        }
    ],
    "evaluator": {
        "func": "compare_csv",
        "result": {
            "type": "docker_file",
            "path": "/workspace/result.csv"
        },
        "expected": {
            "type": "cloud_file",
            "path": "https://drive.usercontent.google.com/download?id=1W-BNOrT8zUsXNupmjchvBXMiyN0PvoGx&export=download&authuser=0&confirm=t",
            "dest": "/workspace/gold.csv"
        }
    }
}