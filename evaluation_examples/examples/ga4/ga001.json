{
    "id": "ga001",
    "source": "https://developers.google.com/analytics/bigquery/advanced-queries?hl=en",
    "image_name": "bash-py-postgres",
    "init_args": {
        "work_dir": "/workspace",
        "ports": {
            "5432": "12001"
        }
    },
    "instruction": "Provide a list of other products bought by customers who purchased a 'Google Navy Speckled Tee' in December 2020, specifying the item names and quantities. Please complete the query code 'query.py'. The schema of this complex database is stored in the google_analytic4 folder.",
    "hardness": "level2",
    "config": [
        {
            "type": "download_and_execute",
            "parameters": {
                "url": "https://drive.usercontent.google.com/download?id=1efAlObIQlcgF2-eedAd1ZzwW8ddAFXUo&export=download&authuser=0&confirm=t",
                "path": "/workspace/init.sh"
            }
        }
    ],
    "evaluator": {
        "func": "compare_csv",
        "result": {
            "type": "docker_file",
            "path": "/workspace/result.csv"
        },
        "expected": {
            "type": "cloud_file",
            "path": "https://drive.usercontent.google.com/download?id=12yDCTIunJyGWzqU3CdKC8BR6RHdS8Wij&export=download&authuser=0&confirm=t",
            "dest": "/workspace/gold.csv"
        }
    }
}